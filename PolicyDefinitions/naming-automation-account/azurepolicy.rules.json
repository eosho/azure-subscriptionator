{
  "if": {
    "allOf": [
      {
        "field": "type",
        "equals": "Microsoft.Automation/automationAccounts"
      },
      {
        "not": {
          "allOf": [
            {
              "anyOf": [
                {
                  "field": "name",
                "match": "[resourceGroup().id]-AA-*"
                }
              ]
            },
            {
              "anyOf": [
                {
                  "field": "name",
                  "match": "*-AA-."
                },
                {
                  "field": "name",
                  "match": "*-AA-.."
                },
                {
                  "field": "name",
                  "match": "*-AA-..."
                },
                {
                  "field": "name",
                  "match": "*-AA-...."
                },
                {
                  "field": "name",
                  "match": "*-AA-....."
                },
                {
                  "field": "name",
                  "match": "*-AA-......"
                },
                {
                  "field": "name",
                  "match": "*-AA-......."
                },
                {
                  "field": "name",
                  "match": "*-AA-........"
                }
              ]
            }
          ]
        }
      }
    ]
  },
  "then": {
  "effect": "[parameters('effect')]"
  }
}