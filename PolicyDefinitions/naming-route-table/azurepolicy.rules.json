{
  "if": {
    "allOf": [
      {
        "field": "type",
        "equals": "Microsoft.Network/routeTables"
      },
      {
        "not": {
          "allOf": [
            {
              "anyOf": [
                {
                  "field": "name",
                "match": "[resourceGroup().id]-VN-.-SN-*"
                },
                {
                  "field": "name",
                "match": "[resourceGroup().id]-VN-..-SN-*"
                },
                {
                  "field": "name",
                "match": "[resourceGroup().id]-VN-...-SN-*"
                },
                {
                  "field": "name",
                "match": "[resourceGroup().id]-VN-....-SN-*"
                },
                {
                  "field": "name",
                "match": "[resourceGroup().id]-VN-.....-SN-*"
                },
                {
                  "field": "name",
                "match": "[resourceGroup().id]-VN-......-SN-*"
                },
                {
                  "field": "name",
                "match": "[resourceGroup().id]-VN-.......-SN-*"
                },
                {
                  "field": "name",
                "match": "[resourceGroup().id]-VN-........-SN-*"
                }
              ]
            },
            {
              "anyOf": [
                {
                  "field": "name",
                  "match": "*-SN-.-RT"
                },
                {
                  "field": "name",
                  "match": "*-SN-..-RT"
                },
                {
                  "field": "name",
                  "match": "*-SN-...-RT"
                },
                {
                  "field": "name",
                  "match": "*-SN-....-RT"
                },
                {
                  "field": "name",
                  "match": "*-SN-.....-RT"
                },
                {
                  "field": "name",
                  "match": "*-SN-......-RT"
                },
                {
                  "field": "name",
                  "match": "*-SN-.......-RT"
                },
                {
                  "field": "name",
                  "match": "*-SN-........-RT"
                }
              ]
            }
          ]
        }
      }
    ]
  },
  "then": {
  "effect": "[parameters('effect')]"
  }
}